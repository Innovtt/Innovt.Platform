<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <PackageId>Innovt.Cloud.AWS.Dynamo</PackageId>
    <PackageVersion>7.0.33</PackageVersion>
    <Authors>Michel,Tiago</Authors>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Copyright>Innovt Tecnologia</Copyright>
    <Description>AWS implementation for DynamoDB Abstraction.</Description>
    <LangVersion>latest</LangVersion><AnalysisMode>AllEnabledByDefault</AnalysisMode>
  </PropertyGroup>
    <ItemGroup>
    <PackageReference Include="AWSSDK.DynamoDBv2" Version="3.7.201.11" />
    <PackageReference Include="Microsoft.CodeAnalysis.NetAnalyzers" Version="7.0.4">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="System.Diagnostics.DiagnosticSource" Version="7.0.2" />
    </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Innovt.Cloud.AWS\Innovt.Cloud.AWS.csproj" />
    <ProjectReference Include="..\Innovt.Domain\Innovt.Domain.csproj" />
  </ItemGroup>

</Project>
