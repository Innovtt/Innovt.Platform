{
  "metadata": [
    {
      "src": [
        {
          "src": "../src",
          "files": [
            "**/*.csproj"
          ],
          "exclude": [
            "**/*Tests.csproj",
            "**/*Test.csproj",
             "**/*Sample.csproj"
          ]
        }
      ],
      "dest": "api",
      "properties": {
        "ProduceReferenceAssembly": true
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "Innovt Platform",
      "_appTitle": "Innovt Platform",
      "_enableSearch": true,
      "pdf": false
    }
  }
}